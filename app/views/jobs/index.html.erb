<h1>Job Listings:</h1>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Company Name</th>
      <th>Job Title</th>
      <th>Location</th>
      <th>Salary Range</th>
    </tr>
  </thead>
  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= link_to job.company_name, job %></td>
        <td><%= job.job_title %></td>
        <td><%= job.location %></td>
        <td><%= job.salary_range %></td>
      </tr>
    <%end%>
  </tbody>
</table>
